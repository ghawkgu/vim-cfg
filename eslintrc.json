{
  "parser": "babel-eslint",
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "settings": {
      "ecmascript": 6,
      "jsx": true,
      "modules": true
    },
    "plugins": [
      "react"
    ],
    "rules": {
      "strict": 1,
      "quotes": 0,
      "no-unused-vars": 0,
      "camelcase": 1,
      "no-underscore-dangle": 0
    }
}
